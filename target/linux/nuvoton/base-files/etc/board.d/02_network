#!/bin/sh
#
# Copyright (C) 2014-2015 OpenWrt.org
#

. /lib/functions/uci-defaults.sh
. /lib/nuc980.sh

board_config_update

case "$(nuc980_board_name)" in

nuc980-iot-gateway)
	ucidef_set_interface_lan "eth0"
	;;
	
nuc980-iot-cm100|\
nuc980-iot-psm100)
	ucidef_set_interfaces_lan_wan "eth1" "eth0"
	;;
	
*)
	ucidef_set_interface_lan "eth0"
	;;

esac

board_config_flush

exit 0
